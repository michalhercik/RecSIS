package home

templ Content(t text) {
    <div id="home-page" class="container pt-3">
        <h1>{ t.Introduction }</h1>
        <p>{ t.IntroductionText }</p>

        <h1>{ t.HowToUse }</h1>
        <p>{ t.HowToUseText }</p>

        <h1>{ t.Authors }</h1>
        <p> @templ.Raw(t.AuthorsText) </p> // TODO make not raw!!!
    </div>
}